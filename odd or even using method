import java.util.*;
class Main {
    void demo(int n){
        if(n%2==0){
            System.out.println("Even");
        }
        else{
         System.out.println("Odd");
        }
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        Main obj=new Main();
        obj.demo(n);

            
    }    
        }
