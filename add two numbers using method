import java.util.*;
class Main {
    int add(int a,int b){
          return(a+b);
    }
    double add(double c,double d){
          return(c+d);
        
    }
    void disp(){
          Scanner sc=new Scanner(System.in);
          int a=sc.nextInt();
          int b=sc.nextInt();
          double c=sc.nextDouble();
          double d=sc.nextDouble();
          System.out.println(add(a,b));
          System.out.println(add(c,d));
}
    public static void main(String[] args) {
        Main obj=new Main();
        obj.disp();
    
        }    
        }
