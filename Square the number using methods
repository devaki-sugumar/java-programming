import java.util.*;
class Main {
    void demo(int n){
        int square=n*n;
         System.out.println(square);
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        Main obj=new Main();
        obj.demo(n);

            
    }    
        }
